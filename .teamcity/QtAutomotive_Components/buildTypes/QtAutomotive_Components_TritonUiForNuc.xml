<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="d5a8f123-3a11-46bf-9a6d-c9460d687171" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/10.0/project-config.xsd">
  <name>Triton UI for NUC</name>
  <description />
  <settings>
    <options>
      <option name="artifactRules" value="+:%system.teamcity.build.tempDir%/_install_/ =&gt; tritonui.tar.gz" />
      <option name="cleanBuild" value="true" />
    </options>
    <parameters />
    <build-runners>
      <runner id="RUNNER_117" name="Build" type="simpleRunner">
        <parameters>
          <param name="script.content"><![CDATA[#!/bin/bash
. %system.teamcity.build.tempDir%/SDK/environment-setup-corei7-64-pelux-linux
qmake
make -j %teamcity.agent.hardware.cpuCount%
make install INSTALL_ROOT=%system.teamcity.build.tempDir%/_install_]]></param>
          <param name="teamcity.build.workingDir" value="Docker/QtAutomotive/Components/TritonUI" />
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="QtAutomotive_Components_HttpsGitLfsQtIoGerritTritonUiGit" />
    </vcs-settings>
    <requirements>
      <equals id="RQ_66" name="system.ubuntu.version" value="16.04" />
      <equals id="RQ_67" name="teamcity.agent.jvm.os.arch" value="amd64" />
    </requirements>
    <build-triggers />
    <build-extensions>
      <extension id="swabra" type="swabra">
        <parameters>
          <param name="swabra.enabled" value="swabra.before.build" />
          <param name="swabra.strict" value="true" />
        </parameters>
      </extension>
      <extension id="jetbrains.agent.free.space" type="jetbrains.agent.free.space">
        <parameters>
          <param name="free-space-work" value="3gb" />
        </parameters>
      </extension>
      <extension id="perfmon" type="perfmon">
        <parameters />
      </extension>
    </build-extensions>
    <cleanup />
  </settings>
</build-type>

