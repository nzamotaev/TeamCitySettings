<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="2996bca1-dd68-4a89-8017-9be79bedb308" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/10.0/project-config.xsd">
  <name>Update manifests to latest meta-qtas-demo</name>
  <description />
  <settings>
    <options>
