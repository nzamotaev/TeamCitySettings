<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="95bb9b3f-a7d5-4a1b-88bc-580d961ff685" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/10.0/project-config.xsd">
  <name>Update/upload packages to repo</name>
  <description />
  <settings>
    <parameters />
    <build-runners>
      <runner id="RUNNER_118" name="do aptly package load" type="simpleRunner">
        <parameters>
          <param name="script.content"><![CDATA[for i in *;do
  /home/teamcity/bin/aptly repo add -force-replace=true qtas_demo ${i}/*.deb
done
/home/teamcity/bin/aptly publish update pelux]]></param>
          <param name="teamcity.build.workingDir" value="/home/nfsexports/bitbake_repo/deb/" />
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings />
    <requirements>
      <equals id="RQ_68" name="system.CanUploadProperty" value="true" />
    </requirements>
    <build-triggers>
      <build-trigger id="TRIGGER_12" type="buildDependencyTrigger">
        <parameters>
          <param name="branchFilter" value="+:&lt;default&gt;" />
          <param name="dependsOn" value="QtAutomotive_Documentation_SdkAndImages_IntelNucSdkAndImage" />
        </parameters>
      </build-trigger>
    </build-triggers>
    <build-extensions>
      <extension id="perfmon" type="perfmon">
        <parameters />
      </extension>
    </build-extensions>
    <cleanup />
  </settings>
</build-type>

